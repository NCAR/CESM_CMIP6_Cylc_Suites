    [[event hooks]]
        shutdown handler = cylc email-suite
[scheduling]
    [[dependencies]]
        graph = """
        {% for I in range(41, 61) %}
                    case_run_2001-06-01__{{I}} => case_st_archive_2001-06-01__{{I}}
                    case_st_archive_2001-06-01__{{I}} => timeseriesL_2001-06-01__{{I}} & ocn_averages_0011-01-01__{{I}}
