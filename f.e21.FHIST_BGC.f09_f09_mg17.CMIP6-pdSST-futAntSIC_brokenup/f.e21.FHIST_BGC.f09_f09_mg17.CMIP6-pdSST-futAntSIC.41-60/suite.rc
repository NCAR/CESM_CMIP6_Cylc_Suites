    {% set d = "/gpfs/fs1/work/cmip6/cases/PAMIP/f.e21.FHIST_BGC.f09_f09_mg17.CMIP6-pdSST-futAntSIC/f.e21.FHIST_BGC.f09_f09_mg17.CMIP6-pdSST-futAntSIC." %}
